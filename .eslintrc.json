{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style":"off",
        "import/no-extraneous-dependencies":"off",
        "no-console": "off",
        "indent": "off",
        "no-trailing-spaces": "off",
        "eol-last": "off",
        "object-curly-newline":"off"
    }
}
